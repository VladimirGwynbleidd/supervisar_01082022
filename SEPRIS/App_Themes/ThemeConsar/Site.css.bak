/* Fin estilos para el ToolTip del GridView */
#tt {
    clear: none;
    display: block;
    position: absolute;
}

#tttop {
    clear: none;
    display: block;
    margin-left: 5px;
    overflow: hidden;
    height: 10px;
}

#ttcont {
    clear: none;
    border-right: medium none;
    padding-right: 12px;
    border-top: medium none;
    display: block;
    padding-left: 7px;
    padding-bottom: 3px;
    font: 11px Arial;
    margin-left: 5px;
    text-transform: none;
    border-left: medium none;
    cursor: hand;
    color: black;
    padding-top: 2px;
    border-bottom: medium none;
    letter-spacing: normal;
    background-color: #D3D3D3;
    text-align: left;
    text-decoration: none;
}

#ttbot {
    clear: none;
    border-right: medium none;
    border-top: medium none;
    display: block;
    font: 11px Arial;
    margin-left: 5px;
    overflow: hidden;
    text-transform: none;
    border-left: medium none;
    cursor: hand;
    color: black;
    border-bottom: medium none;
    letter-spacing: normal;
    height: 5px;
    background-color: #D3D3D3;
    text-align: left;
    text-decoration: none;
}
/* Fin estilos para el ToolTip del GridView  */

