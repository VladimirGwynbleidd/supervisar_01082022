/* Estilos para Texto */

.lista_autocomplete {
    margin: 0px;
    padding: 3px;
    max-height: 300px;
    overflow: auto;
    border: 1px solid black;
    background-color: White;
}

.itemHighlighted {
    background-color: #DEDEDE;
    font-family: Arial;
    font-size: 11px;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    color: #595959;
    text-decoration: none;
}

.txt_gral {
    font-family: Arial;
    font-size: 11px;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    color: #595959;
    text-decoration: none;
}

[disabled].ddlFiltro {
    background: #ccc;
}

.txt_gral_blanco {
    font-family: Arial;
    list-style-image: url(/images/constante/bullet_verde_full.gif);
    font-size: 10px;
    font-style: normal;
    font-weight: bold;
    font-variant: normal;
    text-transform: none;
    color: #FFFFFF;
    text-decoration: none;
}

/* Fin de estilos para Texto */

/* Estilos para botones */
.botones {
    border-right: medium none;
    border-top: medium none;
    font: 11px Arial;
    text-transform: none;
    border-left: medium none;
    cursor: pointer;
    color: white;
    border-bottom: medium none;
    letter-spacing: normal;
    background-color: #696969;
    text-align: center;
    text-decoration: none;
    width: 93px;
    height: 28px;
}

    .botones:hover {
        background-color: #A9A9A9;
        border-color: -moz-use-text-color;
        border-right: medium none;
        border-style: none;
        border-width: medium;
        color: white;
        cursor: pointer;
        font: 11px Arial;
        height: 28px;
        letter-spacing: normal;
        text-align: center;
        text-decoration: none;
        text-transform: none;
        width: 93px;
    }

.botones_x {
    border-style: solid;
    border-width: 1;
    font: 07px Arial;
    text-transform: none;
    cursor: pointer;
    color: white;
    letter-spacing: normal;
    background-color: #045959;
    text-align: center;
    text-decoration: none;
    width: 14;
    height: 14;
    vertical-align: text-top;
}

    .botones_x:hover {
        border-style: solid;
        border-width: 1;
        font: 07px Arial;
        text-transform: none;
        cursor: pointer;
        color: white;
        letter-spacing: normal;
        background-color: #87100C;
        text-align: center;
        text-decoration: none;
        width: 14;
        height: 14;
        vertical-align: text-top;
    }

.botones_disabled {
    border-right: medium none;
    border-top: medium none;
    font: 11px Arial;
    text-transform: none;
    border-left: medium none;
    cursor: default;
    color: white;
    border-bottom: medium none;
    letter-spacing: normal;
    background-color: #696969;
    text-align: center;
    text-decoration: none;
    width: 93px;
    height: 28px;
}

/* Fin estilos para botones */


