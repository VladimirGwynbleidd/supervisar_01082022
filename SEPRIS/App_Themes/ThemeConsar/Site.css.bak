/* Inicia estilos del GridView*/
.RowSeleccionado {
    background-color: #34B68E !important;
}

.SelectedRow td {
    background-color: #C1DAD7;
    color: Fuchsia;
}

.CongelaColumna {
    background-color: #EFEFEF;
    color: Lime;
}

.GridViewEncabezado TH, .GridViewEncabezado TD {
    padding: 5px;
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 7.5px;
    font-weight: bold;
    white-space: nowrap;
    border-right: 1px solid #AAAAAA;
    border-bottom: 1px solid #AAAAAA;
    background-color: #34B68E;
    vertical-align: bottom;
    text-align: left;
    color: White;
}

.GridViewEncabezado TH {
    text-align: center;
}

    .GridViewEncabezado TH A:link, .GridViewEncabezado TD A:link {
        color: White;
    }

    .GridViewEncabezado TH A:hover, .GridViewEncabezado TD A:hover {
        color: White;
    }

    .GridViewEncabezado TH A:visited, .GridViewEncabezado TD A:visited {
        color: White;
    }

    .GridViewEncabezado TH A:active, .GridViewEncabezado TD A:active {
        color: White;
    }

.GridViewContenido TD {
    padding: 5px;
    padding-bottom: 0px;
    padding-top: 0px;
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 9px;
    white-space: nowrap;
    border-right: 1px solid #AAAAAA;
    border-bottom: 1px solid #AAAAAA;
}

.GridViewContenido .Freeze {
    background-color: #EFEFEF;
}

.GridviewScrollItemHover TD {
    padding: 5px;
    white-space: nowrap;
    border-right: 1px solid #AAAAAA;
    border-bottom: 1px solid #AAAAAA;
    background-color: #CCCCCC;
    cursor: pointer;
}

.GridviewScrollItemHover .Freeze {
    background-color: #CCCCCC;
}

.GridviewScrollItemSelected TD {
    padding: 5px;
    white-space: nowrap;
    border-right: 1px solid #AAAAAA;
    border-bottom: 1px solid #AAAAAA;
    background-color: #999999;
    color: #FFFFFF;
}

.GridviewScrollItemSelected .Freeze {
    background-color: #999999;
}

.GridviewScrollPager {
    border-top: 1px solid #AAAAAA;
    background-color: #8FBC8F;
}

    .GridviewScrollPager TD {
        padding-top: 3px;
        font-size: 12px;
        padding-left: 5px;
        padding-right: 5px;
    }

    .GridviewScrollPager A {
        color: #34B68E;
    }

    .GridviewScrollPager SPAN {
        font-size: 14px;
        color: #FFFFFF;
        font-weight: bold;
    }

.GridViewContenidoAlternate TD {
    padding: 5px;
    padding-bottom: 0px;
    padding-top: 0px;
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 9px;
    white-space: nowrap;
    border-right: 1px solid #AAAAAA;
    border-bottom: 1px solid #AAAAAA;
    background-color: #E1E2E2;
}

.GridViewContenidoAlternate .Freeze {
    background-color: #EFEFEF;
}

.GridViewContenido .Wrap {
    white-space: normal;
}

.GridViewContenidoAlternate .Wrap {
    white-space: normal;
}



/*Terminan estilos de GridView*/



