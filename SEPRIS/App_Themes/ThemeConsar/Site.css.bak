/* Inicia estilos para Componente de Ayuda */
.transparencia {
    opacity: .25;
    -moz-opacity: .25;
    filter: alpha(opacity=25);
    float: right;
}

    .transparencia:hover {
        opacity: 1;
        -moz-opacity: 1;
        filter: alpha(opacity=100);
        float: right;
    }
/* Fin estilos para Componente de Ayuda */


